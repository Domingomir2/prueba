{
  "version": 2,
  "builds": [
    { "src": "public/memoir.html", "use": "@vercel/static" },
    { "src": "api/memoir.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/public/memoir.html" }
  ]
}
